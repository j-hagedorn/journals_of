@article{schwartzMeasuringRefinedTheory2022,
	title = {Measuring the {Refined} {Theory} of {Individual} {Values} in 49 {Cultural} {Groups}: {Psychometrics} of the {Revised} {Portrait} {Value} {Questionnaire}},
	volume = {29},
	issn = {1073-1911},
	shorttitle = {Measuring the {Refined} {Theory} of {Individual} {Values} in 49 {Cultural} {Groups}},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC9131418/},
	doi = {10.1177/1073191121998760},
	abstract = {Researchers around the world are applying the recently revised Portrait Value Questionnaire (PVQ-RR) to measure the 19 values in Schwartz’s refined values theory. We assessed the internal reliability, circular structure, measurement model, and measurement invariance of values measured by this questionnaire across 49 cultural groups (N = 53,472) and 32 language versions. The PVQ-RR reliably measured 15 of the 19 values in the vast majority of groups and two others in most groups. The fit of the theory-based measurement models supported the differentiation of almost all values in every cultural group. Almost all values were measured invariantly across groups at the configural and metric level. A multidimensional scaling analysis revealed that the PVQ-RR perfectly reproduced the theorized order of the 19 values around the circle across groups. The current study established the PVQ-RR as a sound instrument to measure and to compare the hierarchies and correlates of values across cultures.},
	number = {5},
	urldate = {2023-03-09},
	journal = {Assessment},
	author = {Schwartz, Shalom H. and Cieciuch, Jan},
	month = jul,
	year = {2022},
	pmid = {33682477},
	pmcid = {PMC9131418},
	pages = {1005--1019},
	file = {PubMed Central Full Text PDF:C\:\\Users\\joshh\\Zotero\\storage\\5JA6D29T\\Schwartz and Cieciuch - 2022 - Measuring the Refined Theory of Individual Values .pdf:application/pdf},
}

@book{moevsMetaphysicsDanteComedy2005,
	address = {Oxford},
	series = {American {Academy} of {Religion} {Reflection} and {Theory} in the {Study} of {Religion}},
	title = {The {Metaphysics} of {Dante}'s {Comedy}},
	isbn = {978-0-19-517461-8},
	shorttitle = {The {Metaphysics} of {Dante}'s {Comedy}},
	url = {https://doi.org/10.1093/0195174615.001.0001},
	abstract = {Dante's metaphysics — his understanding of reality — is very different from our own. To present Dante's ideas about the cosmos, or God, or salvation, or history, or poetry within the context of post-Enlightenment presuppositions, as is usually done, is thus to capture only imperfectly the essence of those ideas. This book argues that the recovery of Dante's metaphysics is essential if we are to resolve what has been called “the central problem in the interpretation of the Comedy. ”That problem is what to make of the Comedy's claim to the “status of revelation, vision, or experiential record — as something more than imaginative literature.” This book offers a sustained treatment of the metaphysical picture that grounds and motivates the Comedy, and of the relation between those metaphysics and Dante's poetics. The book carries this out through an examination of three notoriously complex cantos of the Paradiso, read against the background of the Neoplatonic and Aristotelian tradition from which they arise.},
	publisher = {Oxford University Press USA},
	author = {Moevs, Christian},
	year = {2005},
	file = {Moevs - 2005 - The Metaphysics of Dante's Comedy.pdf:C\:\\Users\\joshh\\Zotero\\storage\\VUZKHWUJ\\Moevs - 2005 - The Metaphysics of Dante's Comedy.pdf:application/pdf;Snapshot:C\:\\Users\\joshh\\Zotero\\storage\\SCJ2IXR7\\MOETMO-5.html:text/html},
}

