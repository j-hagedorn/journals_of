@book{agambenHighestPovertyMonastic2013,
	title = {The {Highest} {Poverty}: {Monastic} {Rules} and {Form}-of-{Life}},
	isbn = {978-0-8047-8674-4},
	shorttitle = {The {Highest} {Poverty}},
	abstract = {The acclaimed philosopher and author of Homo Sacer contemplates the possibility of true human freedom through a deep analysis of monastic stricture.What is a rule, if it appears to become confused with life? And what is a human life, if, in every one of its gestures, of its words, and of its silences, it cannot be distinguished from the rule? It is to these questions that Giorgio Agamben’s new book turns by means of an impassioned reading of the phenomenon of Western monasticism from Pachomius to St. Francis. The Highest Poverty meticulously reconstructs the lives of monks, with their obsessive attention to temporal articulation and to the Rule, to ascetic techniques and to liturgy. But Agamben’s thesis is that the true novelty of monasticism lies not in the confusion between life and norm, but in the discovery of a new dimension, in which “life” is affirmed in its autonomy, and in which the claim of the “highest poverty” and “use” challenges the law in ways that we must still grapple with today. How can we think a form-of-life, that is, a human life released from the grip of law, and a use of bodies and of the world that never becomes an appropriation? How can we think life as something not subject to ownership but only for common use?},
	language = {en},
	publisher = {Stanford University Press},
	author = {Agamben, Giorgio},
	month = apr,
	year = {2013},
	note = {Google-Books-ID: SvOGEDXnHXIC},
	keywords = {Philosophy / Movements / Critical Theory, Philosophy / Movements / Deconstruction, Philosophy / Political, Philosophy / Religious, Philosophy / Social, Political Science / History \& Theory, Religion / Philosophy},
	file = {Agamben - 2013 - The Highest Poverty Monastic Rules and Form-of-Li.pdf:C\:\\Users\\joshh\\Zotero\\storage\\XU3XD3Q9\\Agamben - 2013 - The Highest Poverty Monastic Rules and Form-of-Li.pdf:application/pdf},
